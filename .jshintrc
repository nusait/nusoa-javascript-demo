{
  "predef" : ["define"],
  
  "browser": false,

  "boss"     : true,
  "bitwise"  : true,
  "camelcase": false,
  "curly"    : false,
  "eqeqeq"   : true,
  "esnext"   : true,
  "freeze"   : true,
  "immed"    : true,
  "latedef"  : false,
  "newcap"   : true,
  "noarg"    : true,
  "nonew"    : true,
  "strict"   : false,
  "trailing" : false,
  "undef"    : true,
  "unused"   : false,
  "validthis": true,
  "expr"     : true,


  "indent": 4,
  "maxlen": 80,

  "debug": false,
  "devel": false,
  "node": true,
  "globals": {
    "require": true,
    "module": true,
    "System": true
  }
}
